    "rich_text_zkenCh": {
      "type": "rich-text",
      "blocks": {
        "text_9MEcwb": {
          "type": "text",
          "settings": {
            "text": "<ul><li>Irregular Periods: Many individuals experience infrequent or prolonged menstrual cycles.</li><li>Excess Androgen: Higher levels of male hormones can lead to symptoms like acne, excess facial or body hair (hirsutism), and male-pattern baldness.</li><li>Polycystic Ovaries: Ovaries may become enlarged and contain many small cysts, which can be detected via ultrasound.</li></ul>"
          }
        }
      },
      "block_order": [
        "text_9MEcwb"
      ],
